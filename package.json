{
	"name": "@nfps.dev/rollup-plugin-microweb",
	"version": "0.1.0",
	"description": "Rollup plugin for bundling and minifying NFP TypeScript projects",
	"keywords": [
		"nfp",
		"nfps.dev",
		"secret-network",
		"smart-contract",
		"nft",
		"privacy"
	],
	"homepage": "https://nfps.dev/",
	"repository": "github:nfps.dev/rollup-plugin-microweb",
	"license": "ISC",
	"author": {
		"name": "Blake Regalia",
		"email": "blake.regalia@gmail.com",
		"url": "https://blake-regalia.com/"
	},
	"type": "commonjs",
	"main": "dist/main.cjs",
	"module": "dist/main.mjs",
	"types": "dist/main.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build:cjs": "tsc -p tsconfig.cjs.json && mv dist/main.js dist/main.cjs",
		"build:mjs": "tsc -p tsconfig.json && mv dist/main.js dist/main.mjs",
		"build": "yarn build:cjs && yarn build:mjs"
	},
	"exports": {
		"require": "./dist/main.cjs",
		"import": "./dist/main.mjs"
	},
	"dependencies": {
		"@blake.regalia/belt": "^0.5.2",
		"@rollup/plugin-node-resolve": "^15.0.2",
		"@rollup/plugin-terser": "^0.4.1",
		"@rollup/plugin-typescript": "^11.1.1",
		"rollup-plugin-filesize": "^10.0.0",
		"uglify-js": "^3.17.4"
	},
	"devDependencies": {
		"@blake.regalia/eslint-config-elite": "^0.2.4",
		"@blake.regalia/tsconfig": "^0.0.2",
		"@types/node": "^20.1.5",
		"@types/uglify-js": "^3.17.1",
		"rollup": "^3.21.7",
		"ts-toolbelt": "^9.6.0",
		"typescript": "^5.0.4"
	},
	"publishConfig": {
		"access": "public"
	}
}
